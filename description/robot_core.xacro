<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >



  <!-- BASE LINK -->

  <link name="base_link">

  </link>

  <!-- BASE_FOOTPRINT LINK -->

  <joint name="base_footprint_joint" type="fixed">
      <parent link="base_link"/>
      <child link="base_footprint"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="base_footprint">
  </link>
  
  <!--chassis link-->
  <link name="chassis-link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- from PDF: ~120.02 x 112.87 x 10 mm -->
        <box size="0.12002 0.11287 0.01"/>
      </geometry>
      <material name="brown">
        <color rgba="0.3 0 0.4 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.12002 0.11287 0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.6"/>
      <!-- box inertia about its center: ixx=m*(y^2+z^2)/12 etc. -->
      <inertia ixx="0.000641981845" ixy="0" ixz="0" iyy="0.00072524002" iyz="0" izz="0.001357221865"/>
    </inertial>
  </link>
  <joint name="joint-base-chassis" type="fixed">
    <parent link="base_link"/>
    <child link="chassis-link"/>
    <!-- keep same height reference as your original (raise chassis center 20 mm) -->
    <origin rpy="0 0 0" xyz="0 0 0.02"/>
  </joint>
  <material name="blue">
    <color rgba="0 0.4 0.5 1"/>
  </material>
  <!-- ${x_position}-${y_position}-wheel-link -->
  <link name="front-right-wheel-link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- from PDF: wheel Ã˜~58 mm, width ~12 mm -->
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="3.33375e-05" ixy="0" ixz="0" iyy="3.33375e-05" iyz="0" izz="6.3075e-05"/>
    </inertial>
  </link>
  <joint name="joint-chassis-front-right-wheel" type="continuous">
    <parent link="chassis-link"/>
    <child link="front-right-wheel-link"/>
    <!-- place wheel centers near the corners using new chassis size -->
    <origin rpy="0 0 0" xyz="0.03101 0.051435 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <!-- ${x_position}-${y_position}-wheel-link -->
  <link name="front-left-wheel-link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="3.33375e-05" ixy="0" ixz="0" iyy="3.33375e-05" iyz="0" izz="6.3075e-05"/>
    </inertial>
  </link>
  <joint name="joint-chassis-front-left-wheel" type="continuous">
    <parent link="chassis-link"/>
    <child link="front-left-wheel-link"/>
    <origin rpy="0 0 0" xyz="0.03101 -0.051435 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <!-- ${x_position}-${y_position}-wheel-link -->
  <link name="rare-right-wheel-link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="3.33375e-05" ixy="0" ixz="0" iyy="3.33375e-05" iyz="0" izz="6.3075e-05"/>
    </inertial>
  </link>
  <joint name="joint-chassis-rare-right-wheel" type="continuous">
    <parent link="chassis-link"/>
    <child link="rare-right-wheel-link"/>
    <origin rpy="0 0 0" xyz="-0.03101 0.051435 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <!-- ${x_position}-${y_position}-wheel-link -->
  <link name="rare-left-wheel-link">
    <visual>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.012" radius="0.029"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="3.33375e-05" ixy="0" ixz="0" iyy="3.33375e-05" iyz="0" izz="6.3075e-05"/>
    </inertial>
  </link>
  <joint name="joint-chassis-rare-left-wheel" type="continuous">
    <parent link="chassis-link"/>
    <child link="rare-left-wheel-link"/>
    <origin rpy="0 0 0" xyz="-0.03101 -0.051435 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <gazebo reference="chassis-link">
    <material>Gazebo/DarkGrey</material>
  </gazebo>
   
  <gazebo reference="front-right-wheel-link">
    <mu1>10000.0 </mu1>
    <mu2>10000.0 </mu2>
    <material>Gazebo/Purple</material>
  </gazebo>
  <gazebo reference="front-left-wheel-link">
    <mu1>10000.0 </mu1>
    <mu2>10000.0 </mu2>
    <material>Gazebo/Purple</material>
  </gazebo>
  <gazebo reference="rare-right-wheel-link">
    <mu1>10000.0 </mu1>
    <mu2>10000.0 </mu2>
    <material>Gazebo/Purple</material>
  </gazebo>
  <gazebo reference="rare-left-wheel-link">
    <mu1>10000.0 </mu1>
    <mu2>10000.0 </mu2>
    <material>Gazebo/Purple</material>
  </gazebo>
</robot>
